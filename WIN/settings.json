    "vim.visualModeKeyBindingsNonRecursive": [
    ],
    "vim.insertModeKeyBindingsNonRecursive": [


        {"before": ["j","j"],"after": ["<esc>"]},

    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        // screen spit
        {"before": ["<leader>","v"],"commands": [":vsplit"]},
        {"before": ["<leader>","b"],"commands": [":split"]},
        // clear highlight
        {"before": ["<leader>","c","h",],"commands": [":noh"],},
        // Searching and replace line and file
        {"before": ["<leader>","r","l",],"commands": [":s"],},
        {"before": ["<leader>","r","f",],"commands": [":%s"],},
        // Moving across editor group tabs
        {"before": ["<leader>","u"],"commands": ["workbench.action.navigateEditorGroups"]},
        // move current file to next or previous tab group
        {"before": ["<leader>","h"],"commands": ["workbench.action.moveEditorToNextGroup"]},
        {"before": ["<leader>","g"],"commands": ["workbench.action.moveEditorToPreviousGroup"]},
        // Search through files in whole workspace
        {"before": ["<leader>","f","f"],"commands": ["workbench.action.quickOpen"]},
        // Search through files in the current tab group
        {"before": ["<leader>","f"],"commands": ["workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"]},
        // open the definition in new tab
        {"before": ["<leader>","r","d"],"commands": ["editor.action.revealDefinitionAside"]},
        // Focus on side bar
        {"before": ["<leader>","s"],"commands": ["workbench.action.focusSideBar"]},
        // cmake build project
        {"before": ["<leader>","<leader>","b"],"commands": ["cmake.build"]},
        // Harpoon commands

        { "before": ["<leader>","<leader>","k"],"commands": ["vscode-harpoon.gotoNextHarpoonEditor"],},
        { "before": ["<leader>","<leader>","l"],"commands": ["vscode-harpoon.gotoLastHarpoonEditor"],},
        { "before": ["<leader>","<leader>","d"],"commands": ["vscode-harpoon.removeEditor"],},
        { "before": ["<leader>","<leader>","c"],"commands": ["vscode-harpoon.clearHarpoon"],},
        { "before": ["<leader>","<leader>","m"],"commands": ["vscode-harpoon.addEditor"],},
        { "before": ["<leader>","<leader>","j"],"commands": ["vscode-harpoon.gotoPreviousHarpoonEditor"],},
        { "before": ["<leader>","<leader>","h"],"commands": ["vscode-harpoon.editorQuickPick"],},
        
    ],
    "vim.insertModeKeyBindings": [

        {"before": ["j","j"],"after": ["<esc>"]},

    ],
    "vim.normalModeKeyBindings": [
        // Add line below
        {"before": ["<leader>","j"],"after": ["o","<esc>"]},
        // Add line above
        {"before": ["<leader>","k"],"after": ["shift+o","<esc>"]},
        // Save current file
        {"before": ["<leader>","w"],"commands": ["workbench.action.files.save"]},
    ],
